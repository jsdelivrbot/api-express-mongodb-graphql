{
  "name": "api-express-mongodb-graphql",
  "version": "0.1.0",
  "description": "A sample Node.js app using Express 4, mongodb and Graphql",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon index.js --exec babel-node",
    "lint": "eslint"
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-resolvers": "^1.1.0",
    "apollo-server-express": "^1.3.5",
    "apollo-upload-server": "^5.0.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "jsonwebtoken": "^8.2.1",
    "merge-graphql-schemas": "^1.5.1",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.0.15",
    "mongoose-validator": "^2.0.2",
    "path": "^0.12.7",
    "shortid": "^2.2.8"
  },
  "engines": {
    "node": "8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schervo10/api-express-mongodb-graphql.git"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "graphql"
  ],
  "author": "Sebastian Chervo",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "rimraf": "^2.6.2"
  }
}
